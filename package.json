{
  "name": "eslint-config-adjunct",
  "version": "1.0.0",
  "description": "A midly opinionated collection of the best lesser known esLint plugins that every project should include",
  "main": "index.js",
  "scripts": {
    "ci-test": "npm run test",
    "test": "npm run eslint && npm run prepublishOnly",
    "eslint": "eslint --color  *.{js,md} rules/*.js",
    "eslint:fix": "eslint --color --fix  *.{js,md} rules/*.js",
    "prepublishOnly": "eslint-find-rules --unused --no-core"
  },
  "author": "David J. Bradshaw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidjbradshaw/eslint-config-adjunct.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "dependencies": {
    "npm-check-updates": "^3.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.0",
    "eslint-find-rules": "^3.3.1",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-no-constructor-bind": "^1.2.1",
    "eslint-plugin-no-use-extend-native": "^0.4.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^11.0.0",
    "husky": "^3.0.0",
    "in-publish": "^2.0.0",
    "lint-staged": "^9.0.0",
    "prettier": "^1.16.4",
    "prettier-cli": "^0.1.0",
    "safe-publish-latest": "^1.1.2"
  },
  "peerDependencies": {
    "eslint": "^4.19.1 || >= 5.3.0",
    "eslint-plugin-array-func": ">= 3.1.3",
    "eslint-plugin-eslint-comments": ">= 3.0.1",
    "eslint-plugin-html": ">= 5.0.3",
    "eslint-plugin-json": ">= 1.3.2",
    "eslint-plugin-markdown": ">= 1.0.0",
    "eslint-plugin-no-constructor-bind": ">= 1.2.1",
    "eslint-plugin-no-use-extend-native": ">= 0.4.0",
    "eslint-plugin-only-error": ">= 1.0.2",
    "eslint-plugin-promise": ">= 4.0.1",
    "eslint-plugin-simple-import-sort": ">= 3.0.0",
    "eslint-plugin-sonarjs": ">= 0.3.0",
    "eslint-plugin-switch-case": ">= 1.1.2",
    "eslint-plugin-unicorn": ">= 7.1.0"
  },
  "engines": {
    "node": ">= 6.5"
  }
}
