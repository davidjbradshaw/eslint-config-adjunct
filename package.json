{
  "name": "eslint-config-adjunct",
  "version": "2.0.0",
  "author": "David J. Bradshaw",
  "license": "MIT",
  "description": "A midly opinionated collection of the best lesser known esLint plugins that every project should include",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidjbradshaw/eslint-config-adjunct.git"
  },
  "main": "index.js",
  "scripts": {
    "ci-test": "npm run test",
    "eslint": "eslint --color  *.{js,json,md} rules/*.js",
    "eslint:fix": "eslint --color --fix  *.{js,md} rules/*.js",
    "prepublishOnly": "eslint-find-rules --unused --no-core",
    "test": "npm run eslint && npm run prepublishOnly"
  },
  "keywords": [
    "airbnb",
    "config",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "eslint",
    "eslintconfig",
    "javascript",
    "standard",
    "styleguide"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-array-func": "^3.1.4",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-constructor-bind": "^1.2.8",
    "eslint-plugin-no-secrets": "^0.5.4",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^18.0.1",
    "husky": "^4.2.3",
    "in-publish": "^2.0.1",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "prettier-cli": "^0.1.0",
    "safe-publish-latest": "^1.1.4"
  },
  "engines": {
    "node": ">= 6.5"
  }
}
